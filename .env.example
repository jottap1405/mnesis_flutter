# Mnesis Flutter Application - Environment Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# ============================================================================
# API Configuration
# ============================================================================
# Base URL for the backend API
API_BASE_URL=https://api.mnesis.example.com

# API request timeout in seconds
API_TIMEOUT_SECONDS=30

# ============================================================================
# Supabase Configuration (Backend-as-a-Service)
# ============================================================================
# Get these values from your Supabase project settings
# https://app.supabase.com/project/YOUR_PROJECT/settings/api

# Supabase project URL
# SUPABASE_URL=https://YOUR_PROJECT.supabase.co

# Supabase anonymous/public key (safe to expose in client apps)
# SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ============================================================================
# Feature Flags
# ============================================================================
# Enable/disable analytics tracking
ENABLE_ANALYTICS=false

# Enable/disable crash reporting
ENABLE_CRASH_REPORTING=false

# ============================================================================
# Environment Configuration
# ============================================================================
# Current environment: development, staging, or production
ENVIRONMENT=development

# ============================================================================
# Optional: Future Configuration
# ============================================================================
# Add additional configuration as needed:
# - Third-party API keys (that are safe for client-side)
# - Feature toggles
# - Remote config endpoints
# - CDN URLs